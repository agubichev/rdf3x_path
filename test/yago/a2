select ?name
where {
   ?a <isCalled> ?name .
   ?a <type> "wordnet_actor_109765278" .
   ?a <bornInLocation> ?city .
   ?city <locatedIn> ?country .
   ?country <isCalled> "USA" .
   ?a <actedIn> ?m1 .
   ?m1 <type> "wordnet_movie_106613686" .
   ?m1 <isOfGenre> "Western" .
   ?a <directed> ?m2 .
   ?m2 <type> "wordnet_movie_106613686" .
   ?m2 <isOfGenre> "Crime" .
}

