\timing
select sa.value, sb.value
from rdf.facts a, rdf.facts b, rdf.facts c, rdf.strings sa, rdf.strings sb
where a.predicate=31 and b.predicate=30 and c.predicate=0 and a.object=2397 and b.subject=a.subject and b.object=c.subject and c.object<>1522 and a.subject=sa.id and c.object=sb.id;

-- 31 http://simile.mit.edu/2006/01/ontologies/mods3#origin
-- 2397 info:marcorg/DLC
-- 30 http://simile.mit.edu/2006/01/ontologies/mods3#records
-- 1522 http://simile.mit.edu/2006/01/ontologies/mods3#Text
